pipeline {

    agent { dockerfile {
            filename 'Dockerfile'
        }
    }

    stages {
        stage('git_files') {
            steps {
                git 'https://github.com/andruh8245/Practic_Work_8_5.git'
            }
        }
    }
    
    stages {
        stage('run_files') {
            steps {
                sh 'python3 rfam.py'
            }
        }
    }
}
