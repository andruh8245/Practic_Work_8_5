pipeline {
    agent {dockerfile true}
    stages {
        stage('git_files') {
            steps {
                git branch: 'main', url: 'https://github.com/andruh8245/Practic_Work_8_5.git'
            }
        }
        stage('start_script') {
            steps {
                sh 'python3 rfam.py'
            }
        }
    }
}
